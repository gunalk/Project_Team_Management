{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "lib": [
      "dom",
      "es2015.promise",
      "es5"
    ]
  }
  ,"include":["src/**/*.ts","@types"],
  "exclude":["node_modules","test","dist", "**/*.spec.ts"]
}